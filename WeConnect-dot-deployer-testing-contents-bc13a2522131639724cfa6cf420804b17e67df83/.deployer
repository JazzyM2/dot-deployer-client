{
	"version": "2.0.0",
	"autoupdate": true,
	"assets": false,
	"install": [
		{
			"action": "copy",
			"source": ".deployer",
			"destination": "$USERPROFILE\\Desktop\\DotDeployerFile"
		},
		{
			"action": "copy",
			"source": "README.md",
			"destination": "$USERPROFILE\\Desktop\\ReadMeFile"
		}
	],
	"uninstall": [
		{
			"action": "delete",
			"source": "$USERPROFILE\\Desktop\\DotDeployerFile"
		},
		{
			"action": "delete",
			"source": "$USERPROFILE\\Desktop\\ReadMeFile"
		}
	],
	"documentation": {
		"url": "https://www.google.com"
	},
	"support": {
		"url": "https://www.google.com"
	}
}
